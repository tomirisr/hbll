{
  "name": "room_reservation",
  "version": "1.0.0",
  "private": true,
  "description": "this is a practice app",
  "repository": "bitbucket:byuhbll/room_reservation",
  "scripts": {
    "start": "gulp watch",
    "build": "gulp build",
    "server": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "js": "webpack --mode production",
    "js:watch": "gulp js:watch js",
    "sass": "gulp sass",
    "sass:watch": "gulp sass:watch sass",
    "copy": "gulp copy",
    "copy:watch": "gulp copy:watch copy",
    "clean": "gulp clean",
    "test": "jest --env=jsdom"
  },
  "dependencies": {},
  "devDependencies": {
    "@byuhbll/hbll-dev-server": "^1.0.2",
    "@byuhbll/tslint-config": "^2.0.0",
    "@types/jest": "^23.1.3",
    "@types/webpack-env": "^1.13.6",
    "@types/webpack": "^4.4.4",
    "del": "^3.0.0",
    "gulp-autoprefixer": "^5.0.0",
    "gulp-sass": "^4.0.1",
    "gulp-sourcemaps": "^2.6.4",
    "gulp": "^4.0.0",
    "jest": "^22.4.3",
    "ts-jest": "^22.4.4",
    "ts-loader": "^4.2.0",
    "ts-node": "^7.0.0",
    "tsutils": "2.27.1",
    "typescript": "^2.9.2",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4",
    "webpack": "^4.14.0"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(\\.(test|spec))\\.(tsx?|jsx?)$",
    "roots": [
      "<rootDir>/assets/js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "prettier": {
    "printWidth": 100,
    "tabWidth": 4,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "author": "BYU HBLL"
}
